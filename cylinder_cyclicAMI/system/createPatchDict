FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      createPatchDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

pointSync false;

patches
(
    {
        name lowerwall_cyclicAMI;
        patchInfo
        {
            type cyclicAMI;
            neighbourPatch upperwall_cyclicAMI;
            transform translational;
            separationVector (0 0 0.01);
        }
        constructFrom patches;
        patches (lowerwall);
    }

    {
        name upperwall_cyclicAMI;
        patchInfo
        {
            type cyclicAMI;
            neighbourPatch lowerwall_cyclicAMI;
            transform translational;
            separationVector (0 0 -0.01);
        }
        constructFrom patches;
        patches (upperwall);
    }
);

